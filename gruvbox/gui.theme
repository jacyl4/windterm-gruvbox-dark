ApplicationPalette {
	qproperty-link: #689d6a;
	qproperty-windowDisabled: #282828;
}

QWidget {
	font-family: "GeistMono Nerd Font";
	font-size: 9pt;
	background-color: #282828;
	color: #ebdbb2;
}

QAbstractButton:disabled {
	color: #928374;
}

QAbstractItemView {
	background-color: #282828;
	selection-background-color: transparent;
}
QAbstractItemView:active {
	selection-background-color: #3c3836;
}
QAbstractItemView:!active {
	selection-background-color: #803c3836;
}
QAbstractItemView QLineEdit:focus {
	border: 1px solid #689d6a;
	background-color: #282828;
}
QAbstractItemView QProgressBar {
	max-height: 16px;
	text-align: center;
}
QAbstractItemView QToolButton:hover {
	background-color: #3c3836;
}
QAbstractItemView QWidget {
	background-color: transparent;
}
QAbstractItemView QScrollBar {
	background-color: #282828;
}
QAbstractItemView #TitleLabel, QAbstractItemView #SubtitleLabel {
	color: #ebdbb2;
	selection-color: #83a598;
	selection-background-color: transparent;
}
QAbstractItemView::item {
	color: #ebdbb2;
	padding: 2px;
}
QAbstractItemView::indicator:unchecked {
	image: url($(AppDir)/global/themes/default/images/checkbox-unchecked.svg);
}
QAbstractItemView::indicator:checked {
	image: url($(AppDir)/global/themes/default/images/checkbox-checked.svg);
}

QAbstractSpinBox {
	border: 1px solid #504945;
	padding: 2px;
}
QAbstractSpinBox::disabled {
	border: none;
	background-color: #3c3836;
}
QAbstractSpinBox:focus {
	border: 1px solid #689d6a;
}
QAbstractSpinBox::up-button {
	width: 8px;
	padding: 0px 4px 0px 0px;
	image: url(./global/themes/default/images/button-up.svg);
}
QAbstractSpinBox::down-button {
	width: 8px;
	padding: 0px 4px 4px 0px;
	image: url(./global/themes/default/images/button-down.svg);
}

QListView:item:hover {
	background-color: #3c3836;
}
QListView::indicator {
	position: absolute;
	left: -4px;
	width: 24px;
	height: 24px;
}
QTreeView QLabel {
	padding-top: 2px;
	padding-bottom: 2px;
}
QListWidget, QTreeWidget, QTableWidget {
	border: 1px solid #504945;
}
QListWidget QHeaderView::section,
QTreeWidget QHeaderView::section,
QTableWidget QHeaderView::section {
	background-color: #282828;
}
QTableWidget::item {
	padding-top: 0px;
	padding-bottom: 0px;
}

QCheckBox::indicator {
	width: 24px;
	height: 24px;
}
QCheckBox::indicator:checked {
	image: url($(AppDir)/global/themes/default/images/checkbox-checked.svg);
}
QCheckBox::indicator:unchecked {
	image: url($(AppDir)/global/themes/default/images/checkbox-unchecked.svg);
}
QCheckBox::indicator::disabled {
	image: url($(AppDir)/global/themes/default/images/checkbox-disabled.svg);
}

QComboBox {
	border: 1px solid #504945;
	height: 21px;
	padding: 0px 2px 0px 2px;
}
QComboBox::disabled {
	border: none;
	background-color: #3c3836;
}
QComboBox:focus {
	border: 1px solid #689d6a;
}
QComboBox QLineEdit, QComboBox QLineEdit:focus {
	padding: 0px;
	border: none;
	background-color: transparent;
}
QComboBox::drop-down {
	width: 8px;
	padding-right: 4px;
	image: url($(AppDir)/global/themes/default/images/button-down.svg);
}

QDialog QFrame[frameShape = "4"] {
	color: #504945;
}

QLineEdit {
	padding: 2px;
	border: 1px solid #504945;
}
QLineEdit::disabled {
	border: none;
	background-color: #3c3836;
}
QLineEdit[PlaceHolderText = true] {
	font-style: italic;
}
QLineEdit:focus {
	border: 1px solid #689d6a;
}
QLineEdit > SwitchButton:Checked {
	border: 2px solid #3c3836;
	padding: 4px;
	background-color: #3c3836;
}
QLineEdit > SideButton:hover, QLineEdit > SwitchButton:hover {
	border: +1px solid #d5c4a1;
	background-color: #3c3836;
}
QLineEdit > SideButton:pressed, QLineEdit > SwitchButton:pressed {
	border: +1px solid #d5c4a1;
	background-color: #60d5c4a1;
}
QLineEdit > #CompleterButton {
	padding-top: 4px;
	padding-bottom: 4px;
}
QLineEdit > #CompleterButton:checked {
	border: none;
	background-color: transparent;
}

QMenuBar {
	border-bottom: +1px solid #504945;
	padding-top: 4px;
	padding-bottom: 4px;
}
QMenuBar::item:pressed, QMenuBar::item:selected {
	background-color: #282828;
}
QMenu {
	border: 1px solid #504945;
	background-color: #3c3836;
}
QMenu::item {
	padding: 6px 10px 6px 30px;
	background-color: #3c3836;
}
QMenu::item:default {
	font-weight: bold;
	color: #ebdbb2;
}
QMenu::item:disabled {
	color: #928374;
}
QMenu::item:selected {
	border: none;
	background-color: #689d6a;
}
QMenu::indicator {
	width: 24px;
	height: 24px;
}
QMenu::indicator:non-exclusive:unchecked {
	image: url($(AppDir)/global/themes/default/images/checkbox-unchecked.svg);
}
QMenu::indicator:non-exclusive:checked {
	image: url($(AppDir)/global/themes/default/images/checkbox-checked.svg);
}
QMenu::indicator:exclusive:unchecked {
	image: url($(AppDir)/global/themes/default/images/radiobox-unchecked.svg);
}
QMenu::indicator:exclusive:checked {
	image: url($(AppDir)/global/themes/default/images/radiobox-checked.svg);
}
QMenu::icon {
	margin: 10px;
	width: 24px;
	height: 24px;
}
QMenu::icon:checked {
	position: absolute;
	left: -200px;
}
QMenu::icon:unchecked {
	position: absolute;
	left: -200px;
}
QMenu::separator {
	height: 1px;
	background-color: #282828;
}
QMenu::right-arrow {
	position: relative;
	left: -12px;
}

QPlainTextEdit {
	border: 1px solid #504945;
	padding: 2px;
}
QPlainTextEdit::disabled {
	border: none;
	background-color: #282828;
}
QPlainTextEdit:focus {
	border: 1px solid #689d6a;
}

QProgressBar::chunk:!active {
	background-color: #282828; /* Should same as the normal background color of progressbar chunk. */
}
QProgressBar[state = "Error"]::chunk {
	background-color: #fb4934;
}

QRadioButton::indicator {
	width: 24px;
	height: 24px;
}
QRadioButton::indicator:checked {
	image: url($(AppDir)/global/themes/default/images/radiobox-checked.svg);
}
QRadioButton::indicator:unchecked {
	image: url($(AppDir)/global/themes/default/images/radiobox-unchecked.svg);
}
QRadioButton::indicator::disabled {
	image: url($(AppDir)/global/themes/default/images/radiobox-disabled.svg);
}

QScrollBar {
	border: none;
	background-color: transparent;
	margin: 0px 0px 0px 0px;
}
QScrollBar:horizontal {
	height: 8px;
}
QScrollBar:vertical {
	width: 8px;
}
QScrollBar::handle {
	background-color: #504945;
}
QScrollBar::handle:horizontal {
	min-width: 20px;
}
QScrollBar::handle:vertical {
	min-height: 20px;
}
QScrollBar::handle:hover {
	background-color: #665c54;
}
QScrollBar::handle:disabled {
	background-color: none;
}
QScrollBar::add-line, QScrollBar::sub-line {
	width: 0px;
	height: 0px;
}

QSizeGrip {
	width: 0px;
	height: 0px;
}

QSplitter::handle:horizontal {
	width: +1px;
	background-color: #504945;
}
QSplitter::handle:vertical {
	height: +1px;
	background-color: #504945;
}

QStatusBar QLabel {
	padding-left: 4px;
	padding-right: 4px;
}
QStatusBar[messageLevel = "NormalMessage"], QStatusBar[messageLevel = "NormalMessage"] > * {
	color: #ebdbb2;
}
QStatusBar[messageLevel = "InfoMessage"], QStatusBar[messageLevel = "InfoMessage"] > * {
	background-color: #689d6a;
	color: #ebdbb2;
}
QStatusBar[messageLevel = "WarningMessage"], QStatusBar[messageLevel = "WarningMessage"] > * {
	background-color: #d79921;
	color: #282828;
}
QStatusBar[messageLevel = "ErrorMessage"], QStatusBar[messageLevel = "ErrorMessage"] > * {
	background-color: #fb4934;
	color: #282828;
}

QTabBar::scroller {
	width: 0px;
}
QTabBar::tear {
	image: none;
}
QTabBar::tab {
	background-color: #3c3836;
	border-left: 1px solid #1d2021;
	height: 24px;
	padding-left: 8px;
}
QTabBar::tab::first {
	border-color: transparent;
}
QTabBar::tab:selected {
	color: #ebdbb2;
	border-right: 1x solid #1d2021;
	background-color: #1d2021;
}
QTabBar::tab:!selected:hover {
	background-color: #504945;
}

QToolBar {
	border: 2px solid transparent;
	margin-top: 2px;
	spacing: 2px;
}
QToolBar::separator {
	width: 1px;
	background-color: #928374;
}
QToolBar QLabel {
	padding: 4px;
}

QToolButton:hover {
	background-color: #504945;
}
QToolButton::menu-indicator {
	image: none;
}

QToolTip {
	font-family: "GeistMono Nerd Font";
	color: #ebdbb2;
	background-color: #3c3836;
	border: 1px solid #1d2021;
	padding: 4px;
}

AddressBar > Completer > QListView {
	border: none;
	border-bottom: 1px solid #504945;
	selection-background-color: #3c3836;
}
AddressBar > Completer > QListView #CategoryLabel {
	border: none;
	border-right: +1px solid #504945;
	padding-right: 2px;
}
EditorWindow #CategoryLabel {
	min-width: 130px;
	max-width: 130px;
}
TerminalWindow #CategoryLabel {
	min-width: 104px;
	max-width: 104px;
}
AddressBar > Completer > QListView #CategoryLabel[category = "Opened"] {
	color: #b8bb26;
}
AddressBar > Completer > QListView #CategoryLabel[category = "Recent"] {
	color: #fe8019;
}
AddressBar > Completer > QListView #IconLabel {
	padding: 0px;
}
AddressBar > Completer > QListView #SubtitleLabel {
	color: #83a598;
}
AddressBar > Completer > QListView #TitleLabel {
	margin-top: 4px;
	margin-bottom: 4px;
}
AddressBar > QLineEdit {
	padding: 4px;
	border: none;
	background-color: #3c3836;
}
AddressBar > QLineEdit > BreadButton {
	background-color: transparent;
}
AddressBar > QLineEdit > BreadButton[type = "ArrowButton"], AddressBar > QLineEdit > BreadButton[type = "ChevronButton"] {
	padding-left: -4px;
	padding-right: -4px;
}
AddressBar > QLineEdit > BreadButton[type = "TextButton"] {
	padding-right: -4px;
}
AddressBar > QLineEdit > BreadButton[partial = true] {
	color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0.0 #ebdbb2 stop: 0.5 #d5c4a1 stop: 1.0 #3c3836);
}
AddressBar > QLineEdit > BreadButton[important = false] {
	color: #ebdbb2;
}
AddressBar > QLineEdit > BreadButton:hover, AddressBar > QLineEdit > BreadButton:pressed {
	background-color: #689d6a;
	color: #282828;
}

CommandPalette {
	background-color: #282828;
	border: +1px solid #504945;
}
CommandPalette > QLineEdit {
	font-size: 10pt;
}
CommandPalette > QLineEdit #CountLabel {
	padding-right: 2px;
	background-color: transparent;
}
CommandPalette > QListView {
	background-color: transparent;
	border: none;
}
CommandPalette > QListView::item::selected {
	background-color: #3c3836;
}
CommandPalette > QListView[singleLine=true] #SubtitleLabel {
	color: #928374;
}
CommandPalette > QListView[singleLine=true] #TipLabel {
	margin-top: 4px;
	margin-bottom: 4px;
}

KitDock > QWidget,
KitDock KitPane > QLabel,
KitDock QAbstractItemView,
KitDock TabGroup,
KitDock TabGroup QWidget,
KitDock QTabBar::tab:selected,
KitDock QToolBar,
KitDock QToolBar QWidget {
	background-color: #282828;
}

KitPane > QFrame {
	border: none;
}
KitPane FilterEdit {
	background-color: transparent;
	border: none;
	border-bottom: 1px solid #504945;
}
KitPane FilterEdit:focus {
	border: none;
	border-bottom: 1px solid #689d6a;
}

#LoginWizard > QFrame {
	border: +1px solid #504945;
	background-color: #282828;
}
#LoginWizard > QLabel {
	color: #83a598;
	font-size: 11pt;
	padding: 8px;
}

MessageBar {
	border-top: +1px solid #3c3836;
	border-bottom: +1px solid #3c3836;
	background-color: transparent;
}
MessageBar QWidget {
	background-color: transparent;
}
MessageBar #IconLabel[state = "Critical"] {
	qproperty-pixmap: url($(AppDir)/global/themes/default/images/critical.svg);
}
MessageBar #IconLabel[state = "Information"] {
	qproperty-pixmap: url($(AppDir)/global/themes/default/images/information.svg);
}
MessageBar #IconLabel[state = "Question"] {
	qproperty-pixmap: url($(AppDir)/global/themes/default/images/question.svg);
}
MessageBar #IconLabel[state = "Warning"] {
	qproperty-pixmap: url($(AppDir)/global/themes/default/images/warning.svg);
}
MessageBar #CloseButton {
	border: none;
	image: url($(AppDir)/global/themes/default/images/close-normal.svg);
}
MessageBar #CloseButton:hover {
	image: url($(AppDir)/global/themes/default/images/close-hover.svg);
}

#PaneGroup > QTabBar::tab {
	max-width: 160px;
}

PopupWidget, PopupWidget QWidget {
	background-color: #282828;
}
PopupWidget QLabel[enabled=false] {
	color: #928374;
}

QuickBar #CloseButton, QuickBar #ConfigButton {
	border: none;
}
QuickBar #QuickButton {
	border: none;
	padding: 4px 16px 4px 16px;
}
QuickBar #ActionButton {
	padding: 0px -10px 0px 0px;
}

SearchPane > QLineEdit[state = "Error"] {
	border: 1px solid #fb4934;
}

SideSeparator {
	background-color: #282828;
}

TabBar {
	qproperty-highlightBottomRight: 0px 0px;
	qproperty-highlightTopLeft: 3px 0px;
}

TabGroup QToolButton {
	background-color: transparent;
	border: none;
}
TabGroup #TextLabel {
	padding-left: 4px;
	qproperty-alignment: 'AlignVCenter | AlignLeft';
}
TabGroup #TextLabel[state = "Unactivated"] {
	font-style: italic;
	color: #928374;
}
TabGroup #TextLabel[state = "Warning"] {
	color: #fabd2f;
}
TabGroup #TextLabel[state = "Error"] {
	color: #fb4934;
}
TabGroup #TextLabel[state = "Changed"] {
	color: #83a598;
}
TabGroup #TextLabel[state = "Saved"] {
	color: #b8bb26;
}
TabGroup #CloseButton[state = "Unactivated"] {
	image: url($(AppDir)/global/themes/default/images/close-unactivated.svg);
}
TabGroup #CloseButton[state = "Normal"] {
	image: url($(AppDir)/global/themes/default/images/close-normal.svg);
}
TabGroup #CloseButton[state = "Warning"] {
	image: url($(AppDir)/global/themes/default/images/close-warning.svg);
}
TabGroup #CloseButton[state = "Error"] {
	image: url($(AppDir)/global/themes/default/images/close-error.svg);
}
TabGroup #CloseButton[state = "Changed"] {
	image: url($(AppDir)/global/themes/default/images/close-changed.svg);
}
TabGroup #CloseButton[state = "Saved"] {
	image: url($(AppDir)/global/themes/default/images/close-saved.svg);
}

#ToolBarPopup QLabel {
	border: none;
	border-bottom: 1px solid #928374;
	color: #928374;
	padding-bottom: 4px;
	margin-bottom: 6px;
}
#ToolBarPopup QToolButton[popupMode="1"] {
	margin-left: 20px;
}

TextView CompleteList {
	border: 1px solid #504945;
}
TextView CompleteList::item {
	background-color: #282828;
}
TextView CompleteList::item:selected {
	background-color: #282828;
}
TextView > QFrame {
	border: none;
}
TextView > QProgressBar {
	qproperty-format: "%p% (%vMB / %mMB)";
}
TextView > QAbstractScrollArea QScrollBar {
	background-color: #282828;
	border: none;
	margin: 0px;
}
TextView > QAbstractScrollArea QScrollBar:vertical {
	width: 16px;
}

#TextPreview {
	background-color: #282828;
	border-left: none;
	border-top: +2px solid #b16286;
	border-right: none;
	border-bottom: +2px solid #b16286;
}

TransferBar #CloseButton {
	border: none;
}

TransferLabel {
	border: none;
	border-right: +1px solid #504945;
	qproperty-progressColor: #689d6a; /* Should same as the normal background color of progressbar chunk. */
}
TransferLabel[state = "Error"] {
	qproperty-progressColor: #fb4934; /* Should same as the error background color of progressbar chunk. */
}
TransferLabel:hover {
	background-color: #504945;
}
TransferLabel QWidget {
	background-color: transparent;
}
TransferLabel QToolButton:hover {
	background-color: #504945;
}

#ViewGroup {
	qproperty-showSelectedTabCloseButtonOnly: true;
}
#ViewGroup > QTabBar::tab {
	max-width: 200px;
	min-width: 60px;
}
#ViewGroup > QTabBar::tab:selected:!only-one {
	border-top: 2px solid #689d6a;
}
